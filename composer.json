{
    "name": "truefalse-dev/model-logger",
    "description": "Eloquent model events logger",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Timur Logachov",
            "email": "truefalse.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "ModelLogger\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ModelLogger\\Test\\": "tests"
        },
        "files": [
            "tests/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModelLogger\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^4.1",
        "orchestra/testbench": "^10.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    }
}
